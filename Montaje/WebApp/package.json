{
  "name": "new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:f": "vite dev ./frontend",
    "build:f": "npm run build:client && npm run build:server",
    "build:client": "vite build ./frontend --outDir ../dist/client",
    "build:server": "vite build ./frontend --ssr src/entry-server.tsx --outDir ../dist/server",
    "lint:f": "eslint -c ./frontend/eslint.config.js ./frontend",
    "start:b": "node ./build/index.js",
    "dev:b": "cross-env NODE_ENV=test nodemon ./build/index.js",
    "build:b": "tsc",
    "lint:b": "eslint -c ./eslint.config.js .",
    "lint": "pnpm lint:f && pnpm lint:b",
    "lint:fix": "pnpm lint:f --fix & pnpm lint:b --fix"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "formkit": "^1.6.9",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sirv": "^3.0.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "typescript": "~5.8.3",
    "vite": "^6.2.5"
  }
}